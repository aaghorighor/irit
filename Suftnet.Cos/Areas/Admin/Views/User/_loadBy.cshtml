<div class="tableName">

    <table id="tblUser" class="display" style="width:100%">
        <thead>
            <tr>
                <th align="left">FirstName</th>
                <th align="left">LastName</th>
                <th align="left">Email</th>
                <th align="left">Area</th>
                <th align="left">Active</th>
                <th class="align-center">Actions</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
    <div class="hidden">
        <span id="loadTenantUrl" name="loadTenantUrl" data-loadTenantUrl="@Url.Action("fetch", "User", new { area = "admin", Id = ViewContext.RouteData.Values["queryString"].ToString() })"></span>       
        <span id="permissionUrl" name="permissionUrl" data-permissionUrl="@Url.Action("entry", "permission", new { area = "admin" })"></span>
        <span id="editUrl" name="editUrl" data-editUrl="@Url.Action("edit", "User", new { area = "admin" })"></span>      
        <span id="deleteUrl" name="deleteUrl" data-deleteUrl="@Url.Action("delete", "User", new { area = "admin" })"></span>
        <span id="imageUrl" name="imageUrl" data-imageUrl="@Url.Content("~/Content/Photo/User/216X196/")"></span>
        <span id="defaultImageUrl" name="defaultImageUrl" data-defaultImageUrl="@Url.Content("~/Content/Photo/Blank.jpg")"></span>
    </div>
</div>












