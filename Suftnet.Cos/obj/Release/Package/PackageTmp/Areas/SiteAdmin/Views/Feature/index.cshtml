@using Suftnet.Cos.Extension

@{
    ViewBag.Title = "SUFTNET | FEATURE ";   
}

<div class="widgets" style="display:none ">

    <div class="twoOne">
        <div class="widget">
            <div class="header"><span><span class="ico  gray random"></span>@this.ViewContext.ToTitle((int)Suftnet.Cos.Common.eSettings.Feature) </span></div>
            <div class="content">
                <div class="tableName">
                    @Html.Partial("_loadCommon")
                </div>
            </div><!-- End content -->
        </div><!-- End full width -->
    </div>

    <div class="oneThree">
        <div class="widget">
            <div class="header"><span><span class="ico gray window"></span></span></div>
            <div class="content">
                @Html.Partial("_commonform")
            </div>
        </div>
    </div>

</div>

<script type="text/javascript" >

    $(function () {
              
        setTimeout(function () {
            $(".widgets").show();
        }, 0);
   
    });
</script>





