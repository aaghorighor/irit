@{
    ViewBag.Title = "SUFTNET | LOOKUP ";
}

<ul style="margin-left:0px; margin-top :5px" class="uibutton-group">
    <li><span class="etip"><a href="@Url.Action("index", "ordertype")" class="uibutton icon add normal " title="Order Type">Order Type</a></span></li>
    <li><span class="etip"><a href="@Url.Action("index", "orderstatus")" class="uibutton icon add normal " title="Order Status">Order Status</a></span></li>
    <li><span class="etip"><a href="@Url.Action("index", "paymentMethod")" class="uibutton icon add normal " title="Payment Method">Payment Method</a></span></li>
    <li><span class="etip"><a href="@Url.Action("index", "paymentStatus")" class="uibutton icon add normal " title="Payment Status">Payment Status</a></span></li>
    <li><span class="etip"><a href="@Url.Action("index", "tenantStatus")" class="uibutton icon add normal " title="Tenant Status">Tenant Status</a></span></li>
