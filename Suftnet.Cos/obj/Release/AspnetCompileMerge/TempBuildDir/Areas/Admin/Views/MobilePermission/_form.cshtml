
@using Suftnet.Cos.Common;
@using Suftnet.Cos.Extension;

 <form action="@Url.Action("create", "mobilePermission")" method="post"  id="form">   
     <div class="forms">
         @Html.AntiForgeryToken()
         <input id="UserId" name="UserId" value="@this.ViewContext.RouteData.Values["userString"].ToString()" type="hidden" />
         <input type="hidden" name="Id" id="Id" value="c7a037e8-3045-4996-9e02-1794e08622c6" />

         <div class="grid-12-12">
             <label class="forms-lbl">Page: </label>
             @Html.Dropdown("PermissionId", "validate[required]", (int)eSettings.MobilePermission)
         </div>

         <div class="grid-12-12">
             <ul style="margin-left:0px; margin-top :0px" class="display01">
                 <li class="display01"><span class="etip"><a class="uibutton normal " id="btnSaveChanges" title="Save Changes"><i class="fa fa-save"></i> Save Changes</a></span></li>
                 <li class="display01"><span class="etip"><button type="reset" class="uibutton normal" title="Reset"><i class="fa fa-refresh"></i> Reset</button></span></li>
             </ul>
         </div>

     </div>   
   
</form>








