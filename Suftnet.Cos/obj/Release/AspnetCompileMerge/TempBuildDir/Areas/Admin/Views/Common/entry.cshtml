@{
    ViewBag.Title = "SUFTNET | LOOKUP ";
}

<ul style="margin-left:2px; margin-top :5px" class="uibutton-group">
    <li><span class="etip"><a href="@Url.Action("Index","Lookup")" class="uibutton icon prev normal" title="Back">Back</a></span></li>
</ul>

<div class="widgets displaynone">
    <div class="twoOne">
        <div class="widget">
            <div class="header"><span><span class="ico  gray random"></span>@ViewContext.RouteData.Values["name"].ToString()</span></div>
            <div class="content">
                <div class="tableName">
                    @Html.Partial("_load")
                </div>
            </div>
        </div>
    </div>

    <div class="oneThree">
        <div class="widget">
            <div class="header"><span><span class="ico gray window"></span></span></div>
            <div class="content">
                @Html.Partial("_form")
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>

<script type="text/javascript">

    $(function () {

        setTimeout(function () {
            $(".displaynone").show();
        }, 0);

    });
</script>








