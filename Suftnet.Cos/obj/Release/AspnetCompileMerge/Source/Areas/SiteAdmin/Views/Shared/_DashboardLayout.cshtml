@using Suftnet.Cos.Extension;

<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />

    <title>SUFTNET | SITE ADMIN</title>

    @Html.Partial("_master-header-css")

</head>

<body class="dashborad">
    <div id="alertMessage" class="error"></div>
    <!-- Header -->
    @Html.Partial("_dashboardheader")
    <!-- End Header -->
    <div id="shadowhead"></div>
    <!-- Menu -->
    @Html.Partial("_dashboardmenu")
    <!-- End Menu -->

    <div id="content">
        <div class="inner">
            <div style="margin-top:20px"></div>
            <div class="clear"></div>

            @RenderBody()

            <!-- Footer -->
            @Html.Partial("_footer")
            <!-- End Footer -->

        </div>

        <span style="display :none " id="currencySymbol">@Url.Currency()</span>
        <span id="spriteUrl" name="spriteUrl" data-spriteUrl="@Url.SpriteUrl("sprites2.png")"></span>

    </div>

</body>
</html>

@Html.Partial("_init")