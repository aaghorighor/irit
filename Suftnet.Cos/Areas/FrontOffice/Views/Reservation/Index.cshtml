
@{
    ViewBag.Title = "SUFTNET | RESERVATION ORDER";
}

<div style="margin:20px">

    <ul style="margin-left:0px; margin-top :5px" class="uibutton-group">
        <li><span class="etip"><a href="@Url.Action("index", "dashBoard", new { area = "frontoffice" })" class="uibutton icon prev" title="Order Dashboard">Back</a></span></li>
        <li><span class="etip"><a id="btnOpen" href="javascript:void(0)" class="uibutton icon add normal " title="Add New Reservation">Create</a></span></li>
    </ul>

    <div class="widgets">
        <div class="widget">
            <div class="header"><span><span class="ico gray window" title="View Orders"></span>Reservations</span></div>
            <div class="content">
                <div class="tableName">
                    @Html.Partial("_load")
                </div>
            </div>
        </div>
    </div>

    <div id="reservationDialog" style="display :none">
        <div class="widget">
            <div class="header"><span><span class="ico  gray window" title="Modify or Add New Order"></span></span></div>
            <div class="content">
                @Html.Partial("_form")
            </div>
        </div>
    </div>
</div>

@Html.Partial("_lookUp")

<script type="text/javascript">

    $(function () {
        reservation.pageInit();
    });

</script>







