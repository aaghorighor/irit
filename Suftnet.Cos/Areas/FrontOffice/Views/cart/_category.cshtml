
<div id="categoryContainer">
    <div id="mixedSlider">
        <div class="MS-content" data-bind="foreach: catgories">
            <div class="item">
                <a data-bind='click: $parent.get, value :ID' title="">

                    <div class="imgTitle">
                        <h2 class="blogTitle" data-bind="text : Name"></h2>
                        <img data-bind="attr : { src : ImageUrl()}">
                    </div>
                </a>

            </div>
        </div>
        <div class="MS-controls">
            <a class="MS-left"><i class="fa fa-angle-left" aria-hidden="true"></i></a>
            <a class="MS-right"><i class="fa fa-angle-right" aria-hidden="true"></i></a>
        </div>
    </div>
</div>