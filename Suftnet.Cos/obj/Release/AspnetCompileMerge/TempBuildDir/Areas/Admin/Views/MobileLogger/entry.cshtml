@{
    ViewBag.Title = "SUFTNET | MOBILE LOGGER ";
    Layout = "~/Areas/BackOffice/Views/Shared/_backofficeLayout.cshtml";
}

<ul style="margin-left:0px; margin-top :5px" class="uibutton-group">
    <li><span class="etip"><a href="@Url.Action("index", "dashboard")" class="uibutton icon prev normal" title="Back">Back</a></span></li>
    <li><span class="etip"><a id="clear" href="Javascript:void(0)" class="uibutton normal" title="Clear logs">Clear</a></span></li>
</ul>

<div class="widgets" style="display:none ">
    <div class="widget">

        <div class="header"><span><span class="ico  gray window"></span>Logs</span></div>
        <div class="content">
            @Html.Partial("_load")
        </div>

    </div>
</div>
<script type="text/javascript">

 $(function () {

     mobileLogger.pageInit();

        setTimeout(function () {
            $(".widgets").show();
        }, 0);

     });
</script>




