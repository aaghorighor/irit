
@using Suftnet.Cos.Common;
@using Suftnet.Cos.Extension

<div class="forms" > 
                  
    <div class="grid-6-12">          
        <label class="forms-lbl">DateTime Format: </label>
        @Html.Dropdown("DateTimeFormat", "validate[required]", (int)eSettings.DateTimeformat)
    </div> 

    <div class="grid-6-12">
        <label class="forms-lbl">Currency Symbol: </label>
        @Html.CurrencyDropdown("CurrencyId", "", (int)eSettings.Currency)
    </div>     
    
    <div class="grid-6-12">
        <label class="forms-lbl">Tax Rate: </label>
        <input type="text" name="TaxRate" id="TaxRate" class="forms-txt " size="9" maxlength="9" />
    </div>         
  
</div>     