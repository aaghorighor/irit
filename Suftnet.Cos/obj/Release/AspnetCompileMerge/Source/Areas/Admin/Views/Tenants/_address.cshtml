<div class="forms" style="padding:10px;">

    <div class="grid-12-12">
        <label class="forms-lbl">Address: </label>
        <input type="text" name="CompleteAddress" id="CompleteAddress" class="forms-txt " maxlength="200" />
    </div>

    <div class="grid-6-12">
        <label class="forms-lbl">Town: </label>
        <input type="text" name="Town" id="Town" class="forms-txt" size="50" maxlength="50" required="" />
    </div>

    <div class="grid-6-12">
        <label class="forms-lbl">County: </label>
        <input type="text" name="County" id="County" class="forms-txt" size="50" maxlength="50" required="" />
    </div>

    <div class="grid-6-12">
        <label class="forms-lbl">Post Code: </label>
        <input type="text" name="PostCode" id="PostCode" class="forms-txt" size="50" maxlength="50" />
    </div>

    <div class="grid-6-12">
        <label class="forms-lbl">Country: </label>
        <input type="text" name="Country" id="Country" class="forms-txt" size="50" maxlength="50" required="" />
    </div>

    <div class="clear"></div>
    <div class="grid-6-12">
        <label class="forms-lbl">Status: </label>
        <select class="validate[required]" id="StatusId" name="StatusId" style="padding: 6px;">
            <option value="">-- SELECT --</option>
            <option value="207910E5-F78C-4A9C-ACDB-FA69B26B6AC1">Active</option>
            <option value="5AF7A965-FB5D-4E66-A137-549E5FC9C825">Expired</option>
            <option value="5337780F-041E-4E1F-86B2-9D0B0A81C430">Suspended</option>
            <option value="86F72BE4-29B4-41A9-AC22-588E8F3D5E67">Cancelled</option>
            <option value="C402ED8E-3162-46D5-99A0-0E3655B44879">Trial</option>
        </select>
    </div>

    <div class="hide" id="map-item"></div>
</div>



