@model Suftnet.Cos.DataAccess.TenantDto

<div class="forms" style="padding:10px;" > 

    <div class="grid-12-12">                
            <label class="forms-lbl">Address: </label>  
            <input type="text" name="CompleteAddress" id="CompleteAddress" class="forms-txt " value="@Model.Address.CompleteAddress"  maxlength="200" />                          
    </div>    
   
    <div class="grid-6-12">
        <label class="forms-lbl">County: </label>
        <input type="text" name="County" id="County" class="forms-txt" value="@Model.Address.County" size="50" maxlength="50" /> 
    </div>

    <div class="grid-6-12">
        <label class="forms-lbl">Town: </label>
        <input type="text" name="Town" id="Town" class="forms-txt" value="@Model.Address.Town" size="50" maxlength="50" /> 
    </div> 

    <div class="grid-6-12">
        <label class="forms-lbl">Country: </label>
        <input type="text" name="Country" id="Country" class="forms-txt" value="@Model.Address.County" size="50" maxlength="50" />
    </div>

    <div class="grid-6-12">                
        <label class="forms-lbl">Post Code: </label>  
        <input type="text" name="PostCode" id="PostCode" class="forms-txt"  value="@Model.Address.PostCode" size="50"  maxlength="50" />                          
    </div> 

</div>



